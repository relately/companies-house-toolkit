{
  "name": "@relately/companies-house-transformer",
  "version": "0.0.1",
  "description": "Convert Companies House bulk data products into CSV and JSON",
  "exports": "./lib/index.js",
  "bin": "bin/index.js",
  "scripts": {
    "build": "tsc",
